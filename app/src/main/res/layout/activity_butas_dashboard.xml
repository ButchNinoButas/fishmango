<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/Dashboard"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:background="#659da3"
    tools:context=".ButasDashboard">

    <View
        android:id="@+id/backgroundView1"
        android:layout_width="match_parent"
        android:layout_height="122dp"
        android:background="@drawable/rounded_corners"
        app:layout_constraintBottom_toTopOf="@id/guideline1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="3.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        android:layout_marginTop="121dp"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.16" />
    <View
        android:id="@+id/backgroundView2"
        android:layout_width="match_parent"
        android:layout_height="122dp"
        android:background="@drawable/rounded_corners"
        app:layout_constraintBottom_toTopOf="@id/guideline1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="3.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        android:layout_marginTop="250dp"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.16" />
    <View
        android:id="@+id/backgroundView3"
        android:layout_width="match_parent"
        android:layout_height="122dp"
        android:background="@drawable/rounded_corners"
        app:layout_constraintBottom_toTopOf="@id/guideline2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="3.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        android:layout_marginTop="380dp"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.16" />
    <View
        android:id="@+id/backgroundView4"
        android:layout_width="match_parent"
        android:layout_height="122dp"
        android:background="@drawable/rounded_corners"
        app:layout_constraintBottom_toTopOf="@id/guideline3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="3.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        android:layout_marginTop="510dp"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.16" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="See What's Popular"
        android:textColor="#000000"
        android:textStyle="bold"
        android:textSize="25sp"
        tools:ignore="MissingConstraints"
        android:layout_marginTop="80dp"
        android:layout_marginStart="100dp"
        />


    <TextView
        android:id="@+id/bangus1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bangus"
        android:textColor="#000000"
        android:textStyle="bold"
        android:textSize="25sp"
        tools:ignore="MissingConstraints"
        android:layout_marginTop="140dp"
        android:layout_marginStart="180dp"
       />
    <TextView
        android:id="@+id/bangus2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ratings : 4.5/5"
        android:layout_below="@+id/bangus1"
        android:textColor="#000000"

        android:textSize="17sp"
        tools:ignore="MissingConstraints"

        android:layout_marginStart="180dp"
        />

    <Button
        android:id="@+id/bangus4"
        android:layout_width="90dp"
        android:layout_height="45dp"
        android:layout_below="@+id/bangus1"
        android:layout_marginStart="267dp"
        android:textColor="#051e4a"
        android:layout_marginTop="21dp"
        android:backgroundTint="#41bf5a"
        android:text="View"

        android:textSize="17sp" />

    <Button
        android:id="@+id/bangus3"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:text="Hot"
        android:layout_below="@+id/bangus2"
        android:textColor="#000000"
        android:backgroundTint="#c24f48"
        android:textSize="17sp"
        android:layout_marginStart="180dp"
        />
    <ImageView
        android:layout_width="140dp"
        android:layout_height="140dp"
        android:layout_marginTop="110dp"
        android:src="@drawable/banguss"
        android:layout_marginStart="25dp"/>
    <TextView
        android:id="@+id/til1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tilapia"
        android:textColor="#000000"
        android:textStyle="bold"
        android:textSize="25sp"
        tools:ignore="MissingConstraints"
        android:layout_marginTop="270dp"
        android:layout_marginStart="180dp"
        />
    <TextView
        android:id="@+id/til2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ratings : 4.2/5"
        android:layout_below="@+id/til1"
        android:textColor="#000000"

        android:textSize="17sp"
        tools:ignore="MissingConstraints"

        android:layout_marginStart="180dp"
        />
    <Button
        android:id="@+id/til3"
        android:layout_width="90dp"
        android:layout_height="45dp"
        android:text="View"
        android:layout_below="@+id/til1"

        android:backgroundTint="#41bf5a"
        android:textSize="17sp"
        android:layout_marginStart="267dp"
        android:textColor="#051e4a"
        android:layout_marginTop="21dp"

        />
    <Button
        android:id="@+id/til4"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:text="Hot"
        android:layout_below="@+id/til2"
        android:textColor="#000000"
        android:backgroundTint="#c24f48"
        android:textSize="17sp"
        android:layout_marginStart="180dp"
        />
    <ImageView
        android:layout_width="140dp"
        android:layout_height="140dp"
        android:layout_marginTop="240dp"
        android:src="@drawable/tilapias"
        android:layout_marginStart="25dp"/>
    <ImageView
        android:layout_width="140dp"
        android:layout_height="140dp"
        android:layout_marginTop="370dp"
        android:src="@drawable/tulingannew"
        android:layout_marginStart="25dp"/>
    <TextView
        android:id="@+id/till1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tulingan"
        android:textColor="#000000"
        android:textStyle="bold"
        android:textSize="25sp"
        tools:ignore="MissingConstraints"
        android:layout_marginTop="400dp"
        android:layout_marginStart="180dp"
        />
    <TextView
        android:id="@+id/till2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ratings : 4.6/5"
        android:layout_below="@+id/till1"
        android:textColor="#000000"

        android:textSize="17sp"
        tools:ignore="MissingConstraints"

        android:layout_marginStart="180dp"
        />

    <Button
        android:id="@+id/till3"
        android:layout_width="90dp"
        android:layout_height="45dp"
        android:layout_below="@+id/till1"
        android:layout_marginStart="267dp"
        android:textColor="#051e4a"
        android:layout_marginTop="21dp"

        android:backgroundTint="#41bf5a"
        android:text="View"

        android:textSize="17sp" />

    <Button
        android:id="@+id/till4"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:text="Top"
        android:layout_below="@+id/till2"
        android:textColor="#000000"
        android:backgroundTint="#edbc87"
        android:textSize="17sp"
        android:layout_marginStart="180dp"
        />
    <ImageView
        android:layout_width="140dp"
        android:layout_height="140dp"
        android:layout_marginTop="500dp"
        android:src="@drawable/tamarong1"
        android:layout_marginStart="25dp"/>
    <TextView
        android:id="@+id/tilq1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tamarong"
        android:textColor="#000000"
        android:textStyle="bold"
        android:textSize="25sp"
        tools:ignore="MissingConstraints"
        android:layout_marginTop="530dp"
        android:layout_marginStart="180dp"
        />
    <TextView
        android:id="@+id/tilq2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ratings : 1.2/5"
        android:layout_below="@+id/tilq1"
        android:textColor="#000000"

        android:textSize="17sp"
        tools:ignore="MissingConstraints"

        android:layout_marginStart="180dp"
        />
    <Button
        android:id="@+id/tilq3"
        android:layout_width="90dp"
        android:layout_height="45dp"
        android:text="View"
        android:layout_below="@+id/tilq1"
        android:layout_marginStart="267dp"
        android:textColor="#051e4a"
        android:layout_marginTop="21dp"
        android:backgroundTint="#41bf5a"
        android:textSize="17sp"

        />
    <Button
        android:id="@+id/tilq4"
        android:layout_width="85dp"
        android:layout_height="45dp"
        android:text="New"
        android:layout_below="@+id/tilq2"
        android:textColor="#000000"
        android:backgroundTint="#eef5a4"
        android:textSize="17sp"
        android:layout_marginStart="180dp"
        />


    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottom_navigation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:layout_alignParentBottom="true"
        app:labelVisibilityMode="labeled"
        app:menu="@menu/bottom_navigation" />




</RelativeLayout>